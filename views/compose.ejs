<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/" method="post">
  <div class="form-group">
    <label for="composeTitle">Title</label>
    <input type="text" class="form-control" name="composeTitle" id="composeTitle" autocomplete="off">
  </div>
  <div class="form-group">
    <label for="composePost">Post</label>
    <textarea class="form-control" name="composePost" id="composePost" rows="5" cols="30"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">PUBLISH</button>
</form>



<%- include("partials/footer"); -%>
